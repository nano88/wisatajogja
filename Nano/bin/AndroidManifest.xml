<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nano.nano"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Fb"
            android:label="@string/title_activity_fb" >
        </activity>
        <activity
            android:name=".Twitter"
            android:label="@string/title_activity_twitter" >
        </activity>
        <activity
            android:name=".Web"
            android:label="@string/title_activity_web" >
        </activity>
        <activity
            android:name=".Contact"
            android:label="@string/title_activity_contact" >
        </activity>
        <activity
            android:name=".Profiles"
            android:label="@string/title_activity_profiles" >
        </activity>
        <activity
            android:name=".Gplay"
            android:label="@string/title_activity_gplay" >
        </activity>
        <activity
            android:name=".Gplus"
            android:label="@string/title_activity_gplus" >
        </activity>
    </application>

</manifest>
